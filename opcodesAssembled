100	030000	#register 0 will hold the sum, init it
101	031109	#register 1 points to the numbers to add
102	032001	#register 2 holds the number one.
103	043001	#get next number into register 3
104	113108	#if its zero we're finished
105	050003	#otherwise add it to the sum
106	051002	#add one to register one (next number to load)
107	100103	#go for the next one
108	000000	#all done. sum is in register 0
109	123	#the numbers to add
110	234	
111	345	
112	0	#end of the list